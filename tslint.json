{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "jsdoc-format": false,
    "member-ordering": false,
    "max-line-length": false,
    "one-variable-per-declaration": false,
    "no-bitwise": false,
    "ordered-imports": false,
    "triple-equals": false
  },
  "jsRules":{
    "object-literal-sort-keys": false
  }
}
